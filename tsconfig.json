{
    "compilerOptions": {
        "baseUrl": ".",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noEmit": true,
        "sourceMap": true,
        "strict": true,
        "target": "es5",
        "paths": {
            "@ag_cli/*": [
                "src/*"
            ],
            "@ag_cli/tests/*": [
                "tests/*"
            ]
        },
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2017",
        ]
    }
}
